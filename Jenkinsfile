pipeline{
    agent any
    stages{
        stage('build'){
            steps{
                echo "command  to be executed for build of the project"
            }
        }
        stage('test'){
            steps{
                echo "command  to be executed for test of the project"
            }
        }
        stage('deploy'){
            steps{
                echo "command  to be executed for deploy of the project"
            }
        }
    }
}

